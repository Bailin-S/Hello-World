#include <iostream>

using namespace std;

int main()
{
int gas = 20, MPGtown = 23.5, MPGhighway = 28.9;
int distancetown = gas*MPGtown, distanctancehighway = gas*MPGhighway;
cout << "With 20 gallons of gas you can drive " << distancetown << " miles in the town, and " << distanctancehighway << " miles on the highway.";
}
